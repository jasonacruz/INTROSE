
public class Section{
	
	private String sect_name;
	private int sect_yrlvl;
	
	public void setSectname(String sect_name)
	{
		this.sect_name = sect_name;
	}
	
	public void setSectyear(int sect_yrlvl)
	{
		this.sect_yrlvl = sect_yrlvl;
	}
	
	public String getSectname()
	{
		return sect_name;
	}
	
	public int getYearlvl()
	{
		return sect_yrlvl;
	}
	
}
